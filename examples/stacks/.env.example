################################################################################
# HOMER Environment Configuration
# Secrets and endpoints for modular automation
################################################################################

# === Example Module ===

# Base URL for Example API
EXAMPLE_API_URL=https://api.example.com

# Example API Key or access token
EXAMPLE_API_KEY=your-example-api-key

# Optional: runtime mode for Example (e.g., demo, prod)
EXAMPLE_MODE=demo


# === Another Module ===

# Base endpoint for AnotherModule service
ANOTHER_MODULE_API_URL=https://another.example.net

# Auth token or credential for AnotherModule
ANOTHER_MODULE_TOKEN=your-auth-token


# === HOMER Runtime Config ===

# Deployment environment label (development, staging, production)
HOMER_ENV=development

# Logging level: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO
